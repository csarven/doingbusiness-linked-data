<http://renatostauffer.ch>
    rdfs:label "Renato Stauffer"@en;
    .

doingbusiness-structure:EODB2005
	a qb:DataStructureDefinition ;
	qb:component	[qb:dimension dimension:economy; qb:order 1] ;
	qb:component	[qb:dimension dimension:refPeriod; qb:order 2] ;
	qb:component	[qb:measure measure:distance-to-frontier qb:order 3] ;
	qb:component	[qb:measure measure:rank  qb:order 4] ;
	.

dimension:economy
    a rdf:Property, qb:DimensionProperty ;
    skos:prefLabel "Economy"@en ;
    rdfs:subPropertyOf sdmx-dimension:refArea ;
    qb:concept sdmx-concept:refArea ;
    .

dimension:refPeriod
	a rdf:Property, qb:DimensionProperty ;
    skos:prefLabel "reference period"@en ;
    rdfs:subPropertyOf sdmx-dimension:refPeriod ;
    rdfs:range interval:Interval ;
    qb:concept sdmx-concept:refPeriod ;
    .

measure:distance-to-frontier
	a rdf:Property, qb:MeasureProperty ;
	skos:prefLabel "Distance to frontier"@en ;
	skos:notation "DTF" ;
	rdfs:subPropertyOf sdmx-measure:obsValue ;
    rdfs:range xsd:decimal ;
    skos:definition "Definition goes here..."@en ;
    .

measure:rank
	a rdf:Property, qb:MeasureProperty;
	skos:prefLabel "Rank"@en ;
	rdfs:subPropertyOf sdmx-measure:obsValue ;
    rdfs:range xsd:integer ;
    .

doingbusiness:dataset
    rdfs:label "Doing Business datasets"@en ;
    dcterms:hasPart doingbusiness-dataset:ease-of-doing-business ;
    .

<http://doingbusiness.270a.info/dataset/EODB2005>
    a qb:DataSet ;
    qb:structure doingbusiness-structure:EODB2005 ;
    rdfs:label "Ease of Doing Business 2005"@en ;
    dcterms:isPartOf doingbusiness-dataset:ease-of-doing-business ;
    dcterms:hasPart eodb-2005:topics ;
    dcterms:source <http://www.doingbusiness.org/custom-query> ;
    foaf:page <http://www.doingbusiness.org/custom-query> ;
    .

