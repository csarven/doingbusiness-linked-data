@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-attribute: <http://purl.org/linked-data/sdmx/2009/attribute#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix sdmx-measure: <http://purl.org/linked-data/sdmx/2009/measure#> .
@prefix sdmx-metadata: <http://purl.org/linked-data/sdmx/2009/metadata#> .

@prefix doingbusiness: <http://doingbusiness.270a.info/> .
@prefix measure: <http://doingbusiness.270a.info/measure/> .
@prefix doingbusiness-dataset: <http://doingbusiness.270a.info/dataset/> .
@prefix doingbusiness-structure: <http://doingbusiness.270a.info/structure/> .
@prefix dataset: <http://doingbusiness.270a.info/dataset/> .
@prefix structure: <http://doingbusiness.270a.info/structure/> .
@prefix component: <http://doingbusiness.270a.info/component/> .
@prefix dimension: <http://doingbusiness.270a.info/dimension/> .
@prefix concept: <http://doingbusiness.270a.info/concept/> .
@prefix concept-indicator: <http://doingbusiness.270a.info/concept/indicator/> .
@prefix code: <http://doingbusiness.270a.info/code/> .
@prefix code-indicator: <http://doingbusiness.270a.info/code/indicator/> .
@prefix economy: <http://doingbuinsess.270a.info/code/economy/> .

#
# Starting a business dataset
#

dataset:starting-a-business
	a qb:DataSet ;
	qb:structure structure:starting-a-business ;
    rdfs:label "Dooing Business - Starting a Business dataset"@en ;
    foaf:page <http://www.doingbusiness.org/custom-query> ;
    dcterms:issued "2014-12-04T00:00:00Z"^^xsd:dateTime ; 
    dcterms:modified "2014-12-04T00:00:00Z"^^xsd:dateTime ;
    dcterms:creator <http://renatostauffer.ch> ,
                    <http://csarven.ca/#i> ;
    dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    .

structure:starting-a-business
    a qb:DataStructureDefinition ;
        qb:component component:rank ;
        qb:component component:dtf ;
        qb:component component:procedures ;
        qb:component component:days ;
        qb:component component:cost-of-starting-a-business-in-percent-of-income-per-capita ;
        qb:component component:paid-in-minimal-capital-in-percent-of-income-per-capita ;
        qb:component component:refPeriod ;
        qb:component component:economy ;
        qb:component component:indicator-starting-a-business ;
    .

code-indicator:starting-a-business
    a skos:Concept ;
    skos:inScheme code:indicator ;
    skos:topConceptOf code:indicator ;
    skos:definition "The indicator 'Starting a Business' determines how easy it is to start a business in a certain economy."@en ;
    .

code:indicator
    a skos:ConceptScheme ;
    skos:prefLabel "Indicator Concept Scheme"@en ;
    skos:hasTopConcept code-indicator:starting-a-business ;
    skos:hasTopConcept code-indicator:ease-of-doing-business ; #add all other indicators here
    .

component:indicator-starting-a-business
    a qb:ComponentSpecification ;
    qb:dimension code-indicator:starting-a-business ;
    .

component:economy
    a qb:ComponentSpecification ;
    qb:dimension sdmx-dimension:refArea ;
    .

component:refPeriod
    a qb:ComponentSpecification ;
    qb:dimension sdmx-dimension:refPeriod ;
    .

component:rank
    a qb:ComponentSpecification ;
    qb:measure measure:rank;
    .

component:dtf
    a qb:ComponentSpecification ;
    qb:measure measure:dtf ;
    .

component:procedures
    a qb:ComponentSpecification ;
    qb:measure measure:procedures ;
    .

component:days
    a qb:ComponentSpecification ;
    qb:measure measure:days ;
    .

component:cost-of-starting-a-business-in-percent-of-income-per-capita
    a qb:ComponentSpecification ;
    qb:measure measure:cost-of-starting-a-business-in-percent-of-income-per-capita ;
    .

component:paid-in-minimal-capital-in-percent-of-income-per-capita
    a qb:ComponentSpecification ;
    qb:measure measure:paid-in-minimal-capital-in-percent-of-income-per-capita ;
    .

measure:cost-of-starting-a-business-in-percent-of-income-per-capita
    a qb:MeasureProperty ;
    rdfs:label "Cost of starting a business in percent of income per capita"@en ;
    rdfs:range xsd:decimal ;
    .

measure:paid-in-minimal-capital-in-percent-of-income-per-capita
    a qb:MeasureProperty ;
    rdfs:label "Paid in min. capital in percent of income per capita"@en ;
    rdfs:range xsd:decimal ;
    .

measure:procudures
    a qb:MeasureProperty ;
    rdfs:label "Procedures"@en ;
    rdfs:range xsd:integer ;
    .

measure:dtf
    a qb:MeasureProperty ;
    rdfs:label "Distance to Frontier"@en ;
    rdfs:range xsd:decimal ;
    .

measure:rank
    a qb:MeasureProperty ;
    rdfs:label "Rank"@en ;
    rdfs:range xsd:integer ;
    .

measure:days
    a qb:MeasureProperty ;
    rdfs:label "Days"@en ;
    rdfs:range xsd:integer ;
    .

#
# Ease of doing business dataset
#

dataset:ease-of-doing-business
    a qb:DataSet ;
    qb:structure structure:ease-of-doing-business ;
    rdfs:label "Dooing Business - Ease of Doing Business dataset"@en ;
    foaf:page <http://www.doingbusiness.org/custom-query> ;
    dcterms:issued "2014-12-04T00:00:00Z"^^xsd:dateTime ; 
    dcterms:modified "2014-12-04T00:00:00Z"^^xsd:dateTime ;
    dcterms:creator <http://renatostauffer.ch> ,
                    <http://csarven.ca/#i> ;
    dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    .

structure:starting-a-business
    a qb:DataStructureDefinition ;
        qb:component component:rank;
        qb:component component:overall-dtf;
        qb:component component:refPeriod ;
        qb:component component:economy ;
        qb:component component:indicator-ease-of-doing-business ;
    .

component:indicator-ease-of-doing-business
    a qb:ComponentSpecification ;
    qb:dimension code-indicator:ease-of-doing-business ;
    .

code-indicator:ease-of-doing-business
    a skos:Concept ;
    skos:inScheme code:indicator ;
    skos:topConceptOf code:indicator ;
    skos:definition "The indicator 'Ease of doing business' determines how easy it is to do business in a certain economy."@en ;
    .

component:overall-dtf
    a qb:ComponentSpecification ;
    qb:measure measure:overall-dtf ;
    .

measure:overall-dtf
    a qb:MeasureProperty ;
    rdfs:label "Overall Distance to Frontier"@en ;
    rdfs:range xsd:integer ;
    .

