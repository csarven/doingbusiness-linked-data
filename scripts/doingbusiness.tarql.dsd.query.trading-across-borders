PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX skos: <http://www.w3.org/2004/02/skos/core#> 
PREFIX dcterms: <http://purl.org/dc/terms/> 
PREFIX qb: <http://purl.org/linked-data/cube#> 
PREFIX dataset: <http://doingbusiness.270a.info/dataset/>
PREFIX structure: <http://doingbusiness.270a.info/structure/> 
PREFIX component: <http://doingbusiness.270a.info/component/>  
PREFIX code: <http://doingbusiness.270a.info/code/> 
PREFIX code-indicator: <http://doingbusiness.270a.info/code/indicator/> 
PREFIX economy: <http://doingbuinsess.270a.info/code/economy/>

CONSTRUCT{

dataset:trading-across-borders
    a qb:DataSet ;
    qb:structure structure:trading-across-borders ;
    dcterms:title "Trading Across Borders"@en ;
    dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
.

structure:trading-across-borders
    a qb:DataStructureDefinition ;
    qb:component component:rank ;
    qb:component component:dtf ;
    qb:component component:economy ;
    qb:component component:refPeriod ;
    qb:component component:indicator-trading-across-borders ;
    qb:component component:number-of-documents-to-export ;
    qb:component component:days-to-export ;
    qb:component component:cost-to-export-in-us-dollar-per-container ;
    qb:component component:cost-to-export-in-deflated-us-dollar-per-container ;
    qb:component component:number-of-documents-to-import ;
    qb:component component:days-to-import ;
    qb:component component:cost-to-import-in-us-dollar-per-container ;
    qb:component component:cost-to-import-in-deflated-us-dollar-per-container ;
.

code-indicator:trading-across-borders
    a skos:Concept ;
    skos:inScheme code:indicator ;
    skos:topConceptOf code:indicator ;
    skos:prefLabel "Trading Across Borders"@en ;
    .
    
}
FROM <../data/trading-across-borders.2004.mapping.csv>
WHERE{

}
LIMIT 1
