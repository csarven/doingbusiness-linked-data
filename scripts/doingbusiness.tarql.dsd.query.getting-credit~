PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX skos: <http://www.w3.org/2004/02/skos/core#> 
PREFIX dcterms: <http://purl.org/dc/terms/> 
PREFIX qb: <http://purl.org/linked-data/cube#> 
PREFIX dataset: <http://doingbusiness.270a.info/dataset/>
PREFIX structure: <http://doingbusiness.270a.info/structure/> 
PREFIX component: <http://doingbusiness.270a.info/component/>  
PREFIX code: <http://doingbusiness.270a.info/code/> 
PREFIX code-indicator: <http://doingbusiness.270a.info/code/indicator/> 
PREFIX economy: <http://doingbuinsess.270a.info/code/economy/>

CONSTRUCT{

dataset:getting-credit
    a qb:DataSet ;
    qb:structure structure:getting-credit ;
    dcterms:title "Getting Credit"@en ;
    dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
.

structure:getting-credit
    a qb:DataStructureDefinition ;
    qb:component component:rank ;
    qb:component component:dtf ;
    qb:component component:economy ;
    qb:component component:refPeriod ;
    qb:component component:indicator-getting-credit ;
    qb:component component:strength-of-legal-rights-index ;
    qb:component component:depth-of-credit-information-index ;
    qb:component component:credit-registry-coverage-in-percent-of-adults ;
    qb:component component:credit-bureau-coverage-in-percent-of-adults ;
.

code-indicator:getting-credit
    a skos:Concept ;
    skos:inScheme code:indicator ;
    skos:topConceptOf code:indicator ;
    skos:prefLabel "Getting Credit"@en ;
    .
}
FROM <../data/getting-credit.2004.mapping.csv>
WHERE{

}
LIMIT 1
